#include <OBSE/OBSE.h>

OBSE_PLUGIN_VERSION = []() noexcept {
    OBSE::PluginVersionData v{};
    v.PluginVersion({ ${COMMONLIB_PLUGIN_VERSION_MAJOR}, ${COMMONLIB_PLUGIN_VERSION_MINOR}, ${COMMONLIB_PLUGIN_VERSION_PATCH}, 0 });
    v.PluginName("${COMMONLIB_PLUGIN_NAME}");
    v.AuthorName("${COMMONLIB_PLUGIN_AUTHOR}");
    v.UsesAddressLibrary(${COMMONLIBOB64_OPTION_ADDRESS_LIBRARY});
    v.UsesSigScanning(${COMMONLIBOB64_OPTION_SIG_SCANNING});
    v.IsLayoutDependent(${COMMONLIBOB64_OPTION_LAYOUT_DEPENDENT});
    v.HasNoStructUse(${COMMONLIBOB64_OPTION_NO_STRUCT_USE});
    v.CompatibleVersions({ OBSE::RUNTIME_LATEST });
    return v;
}();
